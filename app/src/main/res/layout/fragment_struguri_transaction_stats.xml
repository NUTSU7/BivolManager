<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.struguri.StruguriTransactionStatsFragment">
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsDateCardView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBuyerCardView"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        app:cardBackgroundColor="@color/material_dynamic_primary40"
        app:cardCornerRadius="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/struguriTranStatsIconImgView"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginRight="5dp"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsDate"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:srcCompat="@drawable/ic_struguri_white" />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsIconImgView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_toEndOf="@id/struguriTranStatsIconImgView"
                android:textAlignment="center"
                android:textSize="25sp"
                android:textColor="@color/material_dynamic_primary90"/>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsBuyerCardView"
        app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsDateCardView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsQuantityCardView"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        app:cardBackgroundColor="@color/material_dynamic_primary50"
        app:cardCornerRadius="15dp"
        android:layout_below="@id/struguriTranStatsDateCardView">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBuyerTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsBuyer"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:text="Cumparator"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBuyer"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsBuyerTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsQuantityCardView"
        app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsBuyerCardView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsMoneyCardView"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        app:cardBackgroundColor="@color/material_dynamic_primary50"
        app:cardCornerRadius="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/struguriTranStatsQuantityLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsQuantityTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsQuantity"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsQuantityNRTextView"
                android:text="Cantitate"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsQuantity"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsQuantityTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/struguriTranStatsQuantityNR"
                app:layout_constraintVertical_chainStyle="spread"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsQuantityNRTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsQuantityNR"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsQuantityTextView"
                app:layout_constraintBottom_toBottomOf="parent"
                android:text="Cantitate - check"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsQuantityNR"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsQuantityNRTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsQuantity"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="20sp"
                android:gravity="center"
                android:textColor="@color/material_dynamic_primary90"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsMoneyCardView"
        app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsQuantityCardView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        app:cardBackgroundColor="@color/material_dynamic_primary50"
        app:cardCornerRadius="15dp"
        android:layout_below="@id/struguriTranStatsBuyerCardView">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/struguriTranStatsMoneyLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsMoneyTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsMoney"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsMoneyNCTextView"
                android:textAlignment="center"
                android:text="Bani"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsMoney"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsMoneyTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/struguriTranStatsMoneyNC"
                app:layout_constraintVertical_chainStyle="spread"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsMoneyNCTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsMoneyNC"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsMoneyTextView"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:text="Bani - check"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsMoneyNC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsMoneyNCTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsMoney"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsPriceCardView"
        app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsMoneyCardView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBoxNrCardView"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5dp"
        app:cardBackgroundColor="@color/material_dynamic_primary50"
        app:cardCornerRadius="15dp"
        android:layout_below="@id/struguriTranStatsBuyerCardView">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/struguriTranStatsPriceLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsPriceTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsPrice"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsPriceNCTextView"
                android:textAlignment="center"
                android:text="Pret"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsPrice"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsPriceTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/struguriTranStatsPriceNC"
                app:layout_constraintVertical_chainStyle="spread"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsPriceNCTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsPriceNC"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsPriceTextView"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:text="Pret - check"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsPriceNC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsPriceNCTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsPrice"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/struguriTranStatsBoxNrCardView"
        app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsPriceCardView"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="5sp"
        app:cardBackgroundColor="@color/material_dynamic_primary50"
        app:cardCornerRadius="15dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBoxNrTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsBoxNr"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="spread"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBoxNRNrTextView"
                android:textAlignment="center"
                android:text="Lazi"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/struguriTranStatsBoxNr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsBoxNrTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBoxNRNr"
                app:layout_constraintVertical_chainStyle="spread"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBoxNRNrTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsBoxNRNr"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsBoxNrTextView"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBoxWeightTextView"
                android:textAlignment="center"
                android:text="Lazi - check"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBoxNRNr"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsBoxNRNrTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsBoxNr"
                app:layout_constraintBottom_toTopOf="@+id/struguriTranStatsBoxWeight"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBoxWeightTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toStartOf="@+id/struguriTranStatsBoxWeight"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintVertical_chainStyle="packed"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsBoxNRNrTextView"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:text="Greutate lada"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>

            <com.google.android.material.textview.MaterialTextView
                android:layout_weight="1"
                android:id="@+id/struguriTranStatsBoxWeight"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@id/struguriTranStatsBoxWeightTextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/struguriTranStatsBoxNRNr"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textColor="@color/material_dynamic_primary90"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>
    

</androidx.constraintlayout.widget.ConstraintLayout>